{
    "name": "laminas/laminas-test",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "test"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-test/",
        "issues": "https://github.com/laminas/laminas-test/issues",
        "source": "https://github.com/laminas/laminas-test",
        "rss": "https://github.com/laminas/laminas-test/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev",
            "dev-develop": "2.5-dev"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "laminas/laminas-console": "self.version",
        "laminas/laminas-dom": "self.version",
        "laminas/laminas-eventmanager": "self.version",
        "laminas/laminas-http": "self.version",
        "laminas/laminas-mvc": "self.version",
        "laminas/laminas-servicemanager": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-uri": "self.version",
        "laminas/laminas-view": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "phpunit/phpunit": "3.7.*"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-console": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-console": "Laminas\\Console component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Test\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-test": "self.version"
    }
}
